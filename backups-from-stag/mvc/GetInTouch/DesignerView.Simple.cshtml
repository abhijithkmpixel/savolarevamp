<h4>Get in touch widget</h4>
<div class="form-group">
    <label for="address">Address</label>
    <input type="text" id="address" ng-model="properties.Address.PropertyValue" class="form-control" />
</div>
<div class="form-group">
    <label for="email">Email</label>
    <input type="email" id="emailaddress" ng-model="properties.Email.PropertyValue" class="form-control" />
</div>
<div class="form-group">
    <label for="phone">Phone Number</label>
    <input type="tel" id="phone" ng-model="properties.PhoneNumber.PropertyValue" class="form-control" />
</div>
<div class="form-group">
    <label for="fax">Fax</label>
    <input type="tel" id="fax" ng-model="properties.Fax.PropertyValue" class="form-control" />
</div>